{
  "name": "react-flow-smart-edge",
  "version": "4.0.0",
  "private": false,
  "keywords": [
    "react",
    "typescript",
    "graph",
    "flow",
    "flowchart",
    "smart",
    "edge",
    "pathfinding",
    "react-flow-smart-edge"
  ],
  "homepage": "https://tisoap.github.io/react-flow-smart-edge/",
  "repository": {
    "type": "git",
    "url": "https://github.com/tisoap/react-flow-smart-edge.git"
  },
  "license": "MIT",
  "author": "Tiso Alvarez Puccinelli",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "build": "npm run build-component && npm run build-storybook",
    "build-component": "vite build",
    "build-storybook": "storybook build",
    "check": "npm run typecheck && npm run spellcheck && npm run eslint && npm run prettier",
    "dev": "vite",
    "eslint": "eslint .",
    "eslint-fix": "eslint . --fix",
    "fix": "npm run eslint-fix && npm run prettier-write",
    "prettier": "prettier --check .",
    "prettier-write": "prettier --write .",
    "preview": "vite preview",
    "spellcheck": "cspell lint --no-progress .",
    "storybook": "storybook dev -p 6006",
    "typecheck": "tsc --noEmit --project tsconfig.app.json"
  },
  "devDependencies": {
    "@eslint-react/eslint-plugin": "1.52.4",
    "@eslint/js": "9.33.0",
    "@storybook/addon-docs": "9.1.2",
    "@storybook/react-vite": "9.1.2",
    "@storybook/testing-library": "0.2.2",
    "@types/react": "19.1.10",
    "@types/react-dom": "19.1.7",
    "@vitejs/plugin-react-swc": "4.0.0",
    "@xyflow/react": "12.8.2",
    "cspell": "9.2.0",
    "eslint": "9.33.0",
    "eslint-config-prettier": "10.1.8",
    "eslint-plugin-prettier": "5.5.4",
    "eslint-plugin-react-hooks": "5.2.0",
    "eslint-plugin-react-refresh": "0.4.20",
    "eslint-plugin-sonarjs": "3.0.4",
    "eslint-plugin-storybook": "9.1.2",
    "globals": "16.3.0",
    "prettier": "3.6.2",
    "react": "19.1.1",
    "react-dom": "19.1.1",
    "storybook": "9.1.2",
    "typescript": "5.9.2",
    "typescript-eslint": "8.39.1",
    "vite": "7.1.2",
    "vite-plugin-dts": "4.5.4"
  },
  "peerDependencies": {
    "@xyflow/react": ">=12",
    "react": ">=18",
    "react-dom": ">=18",
    "typescript": ">=5"
  }
}
